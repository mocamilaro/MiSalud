@page
@{
    ViewData["Title"] = "Inicio";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/css/global.css" />
    <link rel="stylesheet" href="~/css/styleguide.css" />
    <link rel="stylesheet" href="~/css/style.css" />
</head>
<body>
    <div class="element-panel-principal" data-model-id="1:10301">
        <div class="contenido-centrado">
            <div class="div">
                <div class="left-side"></div>
            </div>
            <div class="navbar-simple">
                <img class="img" src="https://c.animaapp.com/xuEWHKzq/img/icon-home-2.svg" />
                <img class="img" src="https://c.animaapp.com/xuEWHKzq/img/icon-bell.svg" />
                <img class="img" src="https://c.animaapp.com/xuEWHKzq/img/icon-setting.svg" />
            </div>
            <div class="title-user-profile">
                <div class="text-wrapper">👋🏻 Hola Julia!</div>
                <div class="profile-image"><img class="image" src="https://c.animaapp.com/xuEWHKzq/img/image@2x.png" /></div>
            </div>
            <div class="cards-container">
                <a href="/recomendaciones" class="card">
                    <div class="horizontal-card">
                        <div class="your-diet-chart">Recomendaciones</div> 
                        <img class="card-image" src="https://c.animaapp.com/xuEWHKzq/img/image-2@2x.png" />
                    </div>
                </a>
                <a href="HistoriaClinica1" class="card">
                    <div class="horizontal-card">
                        <div class="your-diet-chart">Fórmula</div>
                        <img class="card-image" src="https://c.animaapp.com/xuEWHKzq/img/image-1@2x.png" />
                    </div>
                </a>
                <a href="HistoriaClinica" class="card">
                    <div class="horizontal-card">
                        <div class="your-diet-chart">Historial clínico</div>
                        <img class="card-image" src="https://c.animaapp.com/xuEWHKzq/img/image-33@2x.png" />
                    </div>
                </a>
                <a href="/agenda" class="card">
                    <div class="horizontal-card">
                        <div class="your-diet-chart">Agenda</div>
                        <img class="card-image" src="https://c.animaapp.com/xuEWHKzq/img/image-35@2x.png" />
                    </div>
                </a>
                <a href="Paciente" class="card">
                    <div class="horizontal-card">
                        <div class="your-diet-chart">Administración</div>
                        <img class="card-image" src="https://c.animaapp.com/xuEWHKzq/img/image-32@2x.png" />
                    </div>
                </a>
            </div>
        </div>
    </div>
</body>
</html>

